{
  "id": "${appId}",
  "uri": "lb://${appId}",
  "predicates": [
    {
      "name": "Path",
      "args": {
        "pattern": "/${name}/**"
      }
    },
    {
      "name": "Weight",
      "args": {
        "_genkey_0": "${name}",
        "_genkey_1": "100"
      }
    }
  ],
  "filters": [
    {
      "name": "StripPrefix",
      "args": {
        "parts": "1"
      }
    },
    {
      "name": "RewritePath",
      "args": {
        "_genkey_0": "/${name}/(?<path>.*)",
        "_genkey_1": "/$\\{path}"
      }
    },
    {
      "name": "IpWhite",
      "args": {
        "sources": "127.0.0.1"
      }
    }
  ],
  "metadata": {
    "version": "2.0"
  },
  "order": 1
}